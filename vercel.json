{
  "version": 2,
  "builds": [
    { "src": "dist/**/*.html",  "use": "@vercel/static" },
    { "src": "dist/**/*.css",   "use": "@vercel/static" },
    { "src": "dist/**/*.js",    "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/",       "dest": "/dist/index.html" },
    { "src": "/archive","dest": "/dist/archive.html" }
  ]
}
